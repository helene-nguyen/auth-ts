{
  "type": "module",
  "name": "learn-ts",
  "version": "1.0.0",
  "description": "Learn TS",
  "main": "dist/index.js",
  "scripts": {
    "start:BUILD_🏰": "tsc -w",
    "start:RUN_🚀": "nodemon dist/index.js",
    "start:LINT_💥❓❗": "eslint . --ext .ts",
    "watch": "concurrently \"npm:start:*\" -c green.bold,yellow.bold,blue.bold",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll"
  },
  "keywords": [
    "typescript"
  ],
  "author": "yumicode",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.9.0",
    "supertest": "^6.2.4",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "concurrently": "^7.3.0",
    "debug": "^4.3.4",
    "eslint": "^8.23.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8"
  },
  "jest": {
    "forceExit": true,
    "collectCoverage": false,
    "verbose": true,
    "detectOpenHandles": false
  }
